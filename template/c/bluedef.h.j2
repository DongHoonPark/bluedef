#ifndef BLUEDEF_H
#define BLUEDEF_H
#ifdef __cplusplus
extern "C" {
#endif

#include <stdint.h>

{%- for namespace, entries in namespaces.items() %}
// {{ namespace }} Identifiers
{%- for entry in entries %}
#define {{ namespace.upper() }}_{{ entry.symbol }} 0x{{ entry.identifier }}
{%- endfor %}
{%- endfor %}

#ifdef __cplusplus
}
#endif
#endif